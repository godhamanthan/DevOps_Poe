pipeline {
  agent any

  stages {
    stage('Checkout') {
      steps {
        checkout scm
      }
    }

    stage('Build') {
      steps {
        sh 'pip install -r requirements.txt'
        // Additional build steps if needed
      }
    }

    stage('Run') {
      steps {
        sh 'python HelloWorld.py'
      }
    }
  }
}
